    <field  key="nationTotalPV" name="全国昨日总PV" type="sku"/>
    <field  key="nationNoStockPV" name="全国昨日无货PV" type="sku"/>
    <field  key="nationNumStock" name="全国现货库存" type="sku"/>
    <field  key="nationNumStockAmount" name="全国现货库存成本" type="sku"/>
    <field  key="nationUnsalableStockAmount" name="全国滞销库存金额" type="sku"/>

