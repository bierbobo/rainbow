        </measure>

        <measure  key="sfsSalesQuantity" name="预测销量" >
            <field  key="sfsSalesQuantity7" name="近7日预测销量" type="dc,rdc"/>
            <field  key="sfsSalesQuantity14" name="近14日预测销量" type="dc,rdc"/>
            <field  key="sfsSalesQuantity28" name="近28日预测销量" type="dc,rdc"/>
        </measure>

    </topic>


    <topic  key="traffic"  name="流量"  >

        <measure  key="totalPV" name="总PV" >
            <field  key="totalPV1" name="昨日总pv" type="dc,rdc"/>
            <field  key="totalPV7" name="7日总pv" type="dc,rdc"/>
            <field  key="totalPV14" name="14日总pv" type="dc,rdc"/>
            <field  key="totalPV28" name="28日总pv" type="dc,rdc"/>
