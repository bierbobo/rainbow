        </measure>

        <measure  key="wmsRecQtty" name="出库地出库销量" >
            <field  key="wmsRecQtty1" name="昨日出库销量" type="dc,rdc"/>
            <field  key="wmsRecQtty7" name="7日出库销量" type="dc,rdc"/>
            <field  key="wmsRecQtty14" name="14日出库销量" type="dc,rdc"/>
            <field  key="wmsRecQtty28" name="28日出库销量" type="dc,rdc"/>
