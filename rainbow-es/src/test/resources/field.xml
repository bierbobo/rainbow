    <field  key="clickNoStock" name="无货点击量 非图书为0" type="sku"/>
    <field  key="clickCount" name="点击量" type="sku"/>
    <field  key="saleCount" name="7日全国销量(从band表中取的，用于band排名的)" type="sku"/>


