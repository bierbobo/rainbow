            <field  key="inventoryAgingYearAmount_4_5" name="4-5年库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingYearAmount_5_6" name="5-6年库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingYearAmount_6_7" name="6-7年库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingYearAmount7_" name="7年以上库存金额" type="dc,rdc"/>
        </measure>

        <measure  key="purchaseQuantity" name="采购入" >
            <field  key="purchaseQuantity1" name="昨日采购入库" type="dc,rdc"/>
            <field  key="purchaseQuantity7" name="7日采购入库" type="dc,rdc"/>
            <field  key="purchaseQuantity14" name="14日采购入库" type="dc,rdc"/>
            <field  key="purchaseQuantity28" name="28日采购入库" type="dc,rdc"/>
            <field  key="purchaseQuantity30" name="30日采购入库" type="dc,rdc"/>
