    <topic  key="sales"  name="销量"  >
        <measure  key="orderSalesQuantity" name="收货地有效销量" >
            <field  key="orderSalesQuantity1" name="昨日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity7" name="7日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity14" name="14日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity28" name="28日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity30" name="30日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity60" name="60日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity90" name="90日有效销量" type="dc,rdc"/>
            <field  key="orderSalesQuantity7_28" name="7/28有效销量" type="dc,rdc"/>
