            <field  key="inventoryAgingDayAmount_1_7" name="1-7天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_8_15" name="8-15天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_16_30" name="16-30天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_31_60" name="31-60天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_61_90" name="61-90天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_91_120" name="91-120天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_121_150" name="121-150天库存金额" type="dc,rdc"/>
            <field  key="inventoryAgingDayAmount_151_180" name="151-180天库存金额" type="dc,rdc"/>
