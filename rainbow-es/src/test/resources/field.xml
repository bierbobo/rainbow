    <topic  key="ware"  name="商品"  >
        <measure  key="wareInfo" name="商品信息" >
            <field  key="skuId" name="商品编码" type="sku"/>
            <field  key="mainSkuId" name="商品主sku" type="sku"/>
            <field  key="skuName" name="商品名称" type="sku"/>
            <field  key="deptName1" name="一级部门" type="sku"/>
            <field  key="deptName2" name="二级部门" type="sku"/>
            <field  key="deptName3" name="三级部门" type="sku"/>
            <field  key="itemFirstCateName" name="一级分类" type="sku"/>
            <field  key="itemSecondCateName" name="二级分类名称" type="sku"/>
            <field  key="itemThirdCateName" name="三级分类名称" type="sku"/>
            <field  key="brandName" name="品牌名称" type="sku"/>
            <field  key="nationVendorCode" name="全国供应商简码" type="sku"/>
            <field  key="nationVendorName" name="全国供应商名称" type="sku"/>
            <field  key="saleStatus" name="上下柜状态" type="sku"/>
            <field  key="offSaleReason" name="下柜原因" type="sku"/>
            <field  key="buyerName" name="采购员" type="sku"/>
            <field  key="salerName" name="销售员" type="sku"/>
            <field  key="bookingStatus" name="预订状态" type="sku"/>
            <field  key="length" name="商品长度" type="sku"/>
            <field  key="width" name="商品宽度" type="sku"/>
            <field  key="height" name="商品高度" type="sku"/>
            <field  key="weight" name="商品体积" type="sku"/>
            <field  key="wt" name="商品重量" type="sku"/>
            <field  key="distributionName" name="配送中心" type="dc"/>
            <field  key="storeName" name="库房名称" type="dc"/>
            <field  key="rdcName" name="机构名称" type="rdc"/>

            <field  key="reserveStatus" name="预售状态" type="dc,rdc"/>
            <field  key="vendorCode" name="供应商简码" type="dc,rdc"/>
            <field  key="vendorName" name="供应商名称" type="dc,rdc"/>
        </measure>

        <measure  key="specialInfo" name="特殊属性" >
            <field  key="isDropship" name="是否厂商直送" type="sku"/>
            <field  key="isNational" name="是否全国发货" type="sku"/>
            <field  key="isStopNoStock" name="是否售完即止" type="sku"/>

            <field  key="isXnzt" name="是否虚拟组套" type="sku"/>
            <field  key="isShelfLife" name="是否保质期商品" type="sku"/>
            <field  key="isParallel" name="是否平行库存" type="sku"/>
            <!-- 新增-->
            <field  key="isNewFlag" name="是否新品" type="sku"/>
            <field  key="isVcFlag" name="是否虚拟品类" type="sku"/>
            <field  key="isAllowReserveFlag" name="是否预售" type="sku"/>
        </measure>

        <measure  key="timeInfo" name="时间相关" >
            <field  key="shelvesTm" name="上架时间" type="sku"/>
            <field  key="lastSaleTime" name="上柜时间" type="sku"/>
            <field  key="offSaleTime" name="下柜时间" type="sku"/>
            <field  key="firstIntoWhTm" name="首次入库时间" type="sku"/>
            <!-- 新增-->
            <field  key="lastIntoWhTm" name="末次入库时间" type="sku"/>
            <field  key="qgp" name="保质期" type="sku"/>
        </measure>

        <measure  key="priceInfo" name="价格相关" >

            <field  key="jdPrice" name="京东价" type="sku"/>
            <field  key="mktPrice" name="市场价" type="sku"/>
            <!--实时接口-->
            <field  key="frontPrice" name="前台售价" type="sku"/>
            <field  key="whQt" name="仓报价" type="dc,rdc"/>
            <field  key="purchasePrice" name="采购价" type="dc,rdc"/>
            <!--手动计算：前台售价-采购价-->
            <field  key="grossProfit" name="毛利" type="dc,rdc"/>
            <!--手动计算：毛利/前台售价-->
            <field  key="grossProfitRate" name="毛利率" type="dc,rdc"/>
        </measure>

        <measure  key="bandInfo" name="Band" >
            <field  key="orgNationClicksBand" name="全国7天一级点击Band" type="sku"/>
            <field  key="orgNationClicksBand37" name="全国7天三级点击Band" type="sku"/>
            <field  key="orgNationSaleNumBand" name="全国7天一级销量Band" type="sku"/>
            <field  key="orgNationSaleNumBand37" name="全国7天三级销量Band" type="sku"/>
            <field  key="orgNationSaleAmtBand" name="全国7天一级销售额BAND" type="sku"/>
            <field  key="orgSaleNumBand7_1" name="区域7天一级销量Band" type="dc,rdc"/>
            <field  key="orgSaleNumBand7_3" name="区域7天三级销量Band" type="dc,rdc"/>

        </measure>
    </topic>

    <topic  key="stock"  name="库存"  >

        <measure  key="financingInventory" name="财务库存" >
            <field  key="avgMonthStoreMoney" name="全国月至今平均库存金额" type="sku"/>
            <field  key="costNorebateAmountS" name="全国月至今累计仓报价成本" type="sku"/>
            <field  key="storeReturnFinance" name="全国财务存货周转" type="sku"/>
            <field  key="returnCostS" name="全国月至今累计退货仓报价成本" type="sku"/>
        </measure>

        <measure  key="erpStock" name="ERP库存" >

            <field  key="inStockNum" name="现货库存" type="dc,rdc"/>
            <!-- 手动计算：   现货库存成本=现货*仓报价-->
            <field  key="inStockNumCost" name="现货库存成本" type="dc,rdc" />
            <field  key="transferInNum" name="内配入" type="dc,rdc"/>
            <field  key="ztNum" name="内配在途" type="dc,rdc"/>
            <field  key="transferOutNum" name="内配出" type="dc,rdc"/>
            <field  key="numOrderBooking" name="订单预定" type="dc,rdc"/>
            <field  key="numAppBooking" name="已锁定" type="dc,rdc"/>
            <field  key="numNosale" name="不可售" type="dc,rdc"/>
            <!--调用外部ERP实时库存接口  = 现货+内配入+内配在途-订单预订-  内配出-锁定-不可销售-转移预占-内部预占-->
            <field  key="availableStock" name="可用库存" type="dc,rdc"/>
            <field  key="notArriveStock" name="采购未到货" type="dc,rdc"/>
            <!--调用外部ERP实时库存接口  = 现货+在途+内配入+采购未到货-订单预订-内配出-->
            <field  key="canReserveNum" name="可订购数量" type="dc,rdc"/>
            <field  key="stock" name="退供应商在途" type="dc,rdc"/>
            <field  key="availableNumOfDays7" name="7日有货天数" type="dc,rdc"/>
            <field  key="availableNumOfDays14" name="14日有货天数" type="dc,rdc"/>
            <field  key="availableNumOfDays28" name="28日有货天数" type="dc,rdc"/>
            <field  key="availableNumOfDays30" name="30日有货天数" type="dc,rdc"/>
            <field  key="availableNumOfDays60" name="60日有货天数" type="dc,rdc"/>
            <field  key="availableNumOfDays90" name="90日有货天数" type="dc,rdc"/>
        </measure>

        <measure  key="storageInventory" name="仓储库存" >
            <field  key="storageInventory_num_stock" name="实物库存" type="dc,rdc"/>
            <field  key="storageInventory_num_stock_amount" name="实物库存金额" type="dc,rdc"/>
            <field  key="storageInventory_kc_flow" name="件数周转天数" type="dc,rdc"/>
            <field  key="storageInventory_mx_stock" name="慢销件数" type="dc,rdc"/>
            <field  key="storageInventory_mx_amount" name="慢销库存金额" type="dc,rdc"/>
            <field  key="storageInventory_zx_stock" name="滞销件数" type="dc,rdc"/>
            <field  key="storageInventory_zx_amount" name="滞销库存金额" type="dc,rdc"/>
        </measure>


        <measure  key="healthyInventory" name="库龄相关库存" >
            <field  key="inventoryAgingDay_1_7" name="1-7天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_8_15" name="8-15天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_16_30" name="16-30天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_31_60" name="31-60天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_61_90" name="61-90天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_91_120" name="91-120天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_121_150" name="121-150天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_151_180" name="151-180天库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingDay_181_360" name="181-360天库存数量" type="dc,rdc"/>

            <field  key="inventoryAgingYear_1_2" name="1-2年库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingYear_2_3" name="2-3年库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingYear_3_4" name="3-4年库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingYear_4_5" name="4-5年库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingYear_5_6" name="5-6年库存数量" type="dc,rdc"/>
            <field  key="inventoryAgingYear_6_7" name="6-7年库存数量" type="dc,rdc"/>
